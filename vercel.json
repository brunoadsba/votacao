{
  "cleanUrls": true,
  "outputDirectory": ".",
  "rewrites": [
    { "source": "/", "destination": "/deploy.html" },
    { "source": "/admin", "destination": "/deploy.html?admin=true" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
      ]
    }
  ]
}
