{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/admin",
      "dest": "/deploy.html?admin=true"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
